{
  "sisyphus": {
    "model": "zai-coding-plan/glm-4.7",
    "todoBinding": true,
    "contextAwareness": "full"
  },
  "agents": {
    "frontend": "coder",
    "librarian": "researcher",
    "oracle": "reviewer",
    "explore": "ops"
