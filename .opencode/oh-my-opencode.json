  "mcp": {
    "websearch": {
      "type": "remote",
      "url": "https://api.exa.ai/mcp",
      "enabled": false
    },
    "context7": {
      "type": "local",
      "command": ["npx", "-y", "@context7/mcp-server"],
      "enabled": false
    },
    "vibe_kanban": {
      "type": "local",
      "command": ["npx", "-y", "vibe-kanban@latest", "--mcp"],
      "enabled": true,
      "description": "Multi-agent task orchestration with visual kanban board"
    }
  },
