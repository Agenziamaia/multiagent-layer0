{
  "name": "maia-opencode",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "typecheck": "tsc --noEmit",
    "check": "npm run lint && npm run typecheck",
    "init": "bash .opencode/scripts/init.sh",
    "layer": "bash .opencode/scripts/layer.sh",
    "supercharge": "opencode run supercharge",
    "plan": "opencode run plan",
    "ops": "opencode run ops",
    "research": "opencode run research",
    "audit": "opencode run audit",
    "setup:vibe-kanban": "bash .opencode/scripts/setup-vibe-kanban.sh",
    "vibe:start": "npx vibe-kanban",
    "track:handoffs": "node .opencode/scripts/auto-handoff.js track",
    "metrics:show": "node .opencode/scripts/auto-handoff.js metrics",
    "deploy:preview": "bash scripts/deploy.sh --preview",
    "deploy:production": "bash scripts/deploy.sh --production",
    "deploy:release": "bash scripts/deploy.sh --production --release",
    "domain:add": "bash scripts/add-custom-domain.sh"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^8.0.0",
    "@stripe/stripe-js": "^8.6.4",
    "clsx": "^2.1.1",
    "framer-motion": "^12.29.0",
    "next": "^14.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "stripe": "^20.2.0",
    "tailwind-merge": "^2.6.0",
    "zustand": "^4.5.7"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/node": "^20.11.19",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.53.1",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.3.1",
    "autoprefixer": "^10.4.23",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jsdom": "^24.0.0",
    "postcss": "^8.5.6",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.19",
    "typescript": "^5.2.2",
    "vite": "^5.1.0",
    "vitest": "^1.3.1"
  }
}
